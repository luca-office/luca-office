{
  "compilerOptions": {
    "target": "es5",
    "lib": ["ES2019", "es7", "es6", "es5", "dom"],
    "jsxImportSource": "@emotion/react",
    "jsx": "react-jsx",
    "noFallthroughCasesInSwitch": true,
    "noImplicitReturns": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "strict": true,
    "baseUrl": ".",
    "paths": {
      // "i18next-browser-languagedetector": ["type-definitions/i18next-browser-languagedetector"],
      "react-mde": ["type-definitions/react-mde/definitions"]
    }
  }
}
