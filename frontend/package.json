{
  "name": "luca-frontend",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "backoffice",
    "player",
    "shared"
  ],
  "scripts": {
    "start:backoffice": "yarn workspace backoffice start",
    "start:player": "yarn workspace player start",
    "test": "yarn workspace backoffice test && yarn workspace player test && yarn workspace shared test",
    "test:backoffice": "yarn workspace backoffice test",
    "test:player": "yarn workspace player test",
    "test:shared": "yarn workspace shared test",
    "storybook:backoffice": "yarn workspace backoffice storybook -p 9001",
    "storybook:player": "yarn workspace player storybook -p 9002",
    "storybook:shared": "yarn workspace shared storybook -p 9003",
    "lint:backoffice": "yarn workspace backoffice lint",
    "lint:player": "yarn workspace player lint",
    "lint:shared": "yarn workspace shared lint",
    "lint": "eslint --config .eslintrc.js --ignore-path .eslintignore '{.,**}/*.{ts,tsx}'",
    "prettier": "prettier --config .prettierrc --ignore-path .prettierignore --write '{.,**}/*.{ts,tsx}'",
    "clear-jest-cache": "yarn workspace backoffice test --clearCache && yarn workspace player test --clearCache && yarn workspace shared test --clearCache",
    "codegen:backoffice": "yarn workspace backoffice apollo-codegen",
    "codegen:player": "yarn workspace player apollo-codegen",
    "codegen:shared": "yarn workspace shared apollo-codegen",
    "typecheck": "yarn workspace backoffice typecheck && yarn workspace player typecheck && yarn workspace shared typecheck",
    "typecheck:backoffice": "yarn workspace backoffice typecheck",
    "typecheck:player": "yarn workspace player typecheck",
    "typecheck:shared": "yarn workspace shared typecheck"
  },
